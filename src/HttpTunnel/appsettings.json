{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Forward": {
    "TunnelHost": "localhost",
    "TunnelPort": 6000,
    "Apps": [
      {
        "Port": 6001,
        "Path": "/A"
      }
    ],
    "Redirects": [
      {
        "From": {
          "Port": 6001,
          "Path": "/B"
        },
        "To": {
          "Port": 7001,
          "Path": "/B"
        }
      }
    ]
  },
  "Backward": {
    "TunnelPort": 6000,
    "Apps": [
      {
        "Port": 6001,
        "Path": "/B"
      }
    ],
    "Redirects": [
      {
        "From": {
          "Port": 6001,
          "Path": "/A"
        },
        "To": {
          "Port": 7001,
          "Path": "/A"
        }
      }
    ]
  }
}
