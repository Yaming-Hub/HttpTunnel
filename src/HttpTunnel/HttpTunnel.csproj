<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <Authors>Yaming Liu</Authors>
    <Company>Microsoft</Company>
    <Description>Http tunnel is a test tool which allows application to talk to remote server as local server via http by setting up a two way http tunnel.</Description>
    <PackageProjectUrl>https://github.com/Yaming-Hub/HttpTunnel</PackageProjectUrl>
    <RepositoryUrl>https://github.com/Yaming-Hub/HttpTunnel</RepositoryUrl>
    <IsPackable>true</IsPackable>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Configuration" Version="5.0.0" />
    <PackageReference Include="Microsoft.Extensions.Configuration.CommandLine" Version="5.0.0" />
    <PackageReference Include="Microsoft.Extensions.Configuration.EnvironmentVariables" Version="5.0.0" />
    <PackageReference Include="Microsoft.Extensions.Configuration.Json" Version="5.0.0" />
    <PackageReference Include="NLog.Web.AspNetCore" Version="4.12.0" />
  </ItemGroup>

</Project>
